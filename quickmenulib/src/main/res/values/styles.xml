<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="GridLayout">
        <item name="android:layout_columnWeight" tools:targetApi="lollipop">1</item>
        <item name="android:layout_rowWeight" tools:targetApi="lollipop">1</item>
        <item name="android:contentDescription" tools:targetApi="lollipop">@string/none_img</item>
    </style>

    <style name="QuickMenuText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:fontFamily" tools:targetApi="jelly_bean">@font/notosanscjkkrmedium</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="QuickSystemIcon">
        <item name="android:layout_width">63dp</item>
        <item name="android:layout_height">63dp</item>
        <item name="android:contentDescription">@string/none_img</item>
    </style>

</resources>